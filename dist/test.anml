(def AndGate
  (Rectangle
    (width 30)
    (height 40)
  )
  (Circle
    (x 30)
    (y 20)
    (radius 20)
  )
)

(def NotGate
  (Triangle
    (vertices
      ((x -20) (y -20))
      ((x 20) (y 0))
      ((x -20) (y 20))
    )
  )
  (Circle
    (x 22)
    (radius 3)
  )
)

(AndGate
  (y 100)
)

(NotGate
  (x 100)
  (y 100)
)

