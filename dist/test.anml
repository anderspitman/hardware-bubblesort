(def AndGate
  (Rectangle
    (width 30)
    (height 40)
  )
  (Circle
    (x 30)
    (y 20)
    (radius 20)
  )
)

(def NotGate
  (Triangle
    (vertices
      ((x -15) (y -15))
      ((x 15) (y 0))
      ((x -15) (y 15))
    )
  )
  (Circle
    (x 18)
    (radius 4)
  )
)

(def GreaterThan
  (AndGate
    (x 60)
  )
  (NotGate
    (x 15)
    (y 32)
  )
)
(GreaterThan
  (x 50)
  (y 50)
)

